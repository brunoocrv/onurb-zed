{
  "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
  "name": "onurb",
  "author": "brunoocrv <brn.carvalho@icloud.com>",
  "themes": [
    {
      "name": "onurb",
      "appearance": "dark",
      "style": {
        "border": "#2A2A2A",
        "border.variant": "#383838",
        "border.focused": "#444",
        "border.selected": "#383838",
        "text": "#FFFFFF",
        "text.muted": "#727272",
        "text.placeholder": "#6b737c",
        "text.disabled": "#444444",
        "text.accent": "#C1C1C1",
        "editor.foreground": "#888888",
        "background": "#1f1f1f",
        "editor.background": "#1f1f1f",
        "editor.active_line.background": "#1f1f1f",
        "editor.line_number": "#727272",
        "editor.active_line_number": "#FAFAFA",
        "icon": "#888888",
        "icon.muted": "#727272",
        "icon.disabled": "#444444",
        "icon.accent": "#C1C1C1",
        "scrollbar.thumb.background": "#2A2A2A",
        "scrollbar.thumb.hover_background": "#383838",
        "toolbar.background": "#1f1f1f",
        "tab.active_background": "#1f1f1f",
        "tab.active_foreground": "#FAFAFA",
        "tab.inactive_background": "#1A1A1A",
        "tab.inactive_foreground": "#727272",
        "status_bar.background": "#1A1A1A",
        "status_bar.foreground": "#7E7E7E",
        "created": "#50fa7b",
        "ignored": "#444444",
        "modified": "#f1fa8c",
        "editor.gutter.background": "#1f1f1f",
        "error": "#ff5555",
        "error.background": "#1f1f1f",
        "error.border": "#ff5555",
        "warning": "#f1fa8c",
        "warning.background": "#1f1f1f",
        "warning.border": "#f1fa8c",
        "info": "#60a5fa",
        "info.border": "#60a5fa",
        "info.background": "#1f1f1f",
        "terminal.background": "#1f1f1f",
        "terminal.ansi.background": "#FAFAFA",
        "surface.background": "#1f1f1f",
        "elevated_surface.background": "#1f1f1f",
        "hint": "#444444",
        "players": [
          {
            "cursor": "#444444",
            "background": "#444444",
            "selection": "#444444"
          }
        ],
        "syntax": {
          "attribute": { "color": "#50fa7b" },
          "comment": { "color": "#6b737c", "font_style": "italic" },
          "constant": { "color": "#fab1e6" },
          "constructor": { "color": "#b392f0" },
          "keyword": { "color": "#5cf6d6" },
          "function": { "color": "#f0abfc" },
          "label": { "color": "#79b8ff" },
          "link_text": { "color": "#1976D2" },
          "number": { "color": "#bd93f9" },
          "boolean": { "color": "#bd93f9" },
          "property": { "color": "#deb8fa" },
          "string": { "color": "#f1fa8c" },
          "string.escape": { "color": "#FF9800" },
          "tag": { "color": "#f0abfc", "font_style": "italic" },
          "type": { "color": "#bd93f9", "font_style": "italic" },
          "emphasis": { "font_style": "italic" },
          "strong": { "color": "#FF7A84" },
          "variable": { "color": "#FFFFFF" },
          "variable.special": { "color": "#deb8fa", "font_style": "italic" }
        }
      }
    }
  ]
}
